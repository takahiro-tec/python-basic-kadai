      "source": [
        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def print_info(self):\n",
        "\n",
        "    print(f\"名前: {self.name}\")\n",
        "    print(f\"年齢: {self.age}\")\n",
        "\n",
        "# Humanクラスのインスタンスを作成\n",
        "person = Human(\"侍太郎\", 30)\n",
        "\n",
        "\n",
        "# 情報を出力\n",
        "person.print_info()\n"
      ]
